{
  "name": "rrule",
  "version": "2.2.8",
  "description": "JavaScript library for working with recurrence rules for calendar dates.",
  "homepage": "http://jakubroztocil.github.io/rrule/",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "keywords": [
    "dates",
    "recurrences",
    "calendar",
    "icalendar",
    "rfc"
  ],
  "author": "Jakub Roztocil and Lars Sch√∂ning",
  "main": "dist/rrule",
  "repository": {
    "type": "git",
    "url": "git://github.com/jakubroztocil/rrule.git"
  },
  "scripts": {
    "build": "npm run standard && webpack",
    "fix": "npm run standard --fix",
    "standard": "standard src/**/*.js test/**/*.js",
    "test": "npm run standard && TZ=UTC mocha --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.1",
    "copy-webpack-plugin": "^4.5.2",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "mocha": "^5.2.0",
    "standard": "^11.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "standard": {
    "ignore": [
      "demo"
    ]
  },
  "files": [
    "lib",
    "README.md"
  ]
}
